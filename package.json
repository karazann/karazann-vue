{
    "name": "karazann-vue",
    "version": "0.1.0",
    "description": "My super-excellent Nuxt.js project",
    "author": "szkabaroli",
    "private": true,
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watchAll",
        "test:cover": "jest --coverage --colors",
        "clear": "rm -rf dist",
        "start:dev": "nuxt-ts",
        "start": "nuxt-ts start",
        "build": "npm run clear && nuxt-ts build",
        "build:gen": "npm run clear && nuxt-ts generate",
        "format-vue": "prettier --write \"src/**/*.vue\" --parser vue --vue-indent-script-and-style",
        "format-ts": "prettier --write \"src/**/*.ts\"",
        "format": "npm run format-vue && npm run format-ts"
    },
    "dependencies": {
        "@abraham/reflection": "^0.7.0",
        "@bit/szkabaroli.karazann-shared.flow": "file:./src/shared/flow",
        "@bit/szkabaroli.karazann-shared.interfaces": "^0.6.0",
        "@bit/szkabaroli.karazann-shared.validator": "^0.4.0",
        "@bit/szkabaroli.karazann-shared.validators": "^0.4.2",
        "@vue/composition-api": "^0.3.4",
        "axios": "^0.19.0",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.11.0",
        "css-vars-ponyfill": "^2.1.2",
        "feather-icons": "^4.25.0",
        "jquery": "^3.4.1",
        "jquery-slim": "^3.0.0",
        "jwt-decode": "^2.2.0",
        "reflect-metadata": "^0.1.13",
        "regenerator-runtime": "^0.13.3",
        "typescript": "^3.7.4",
        "vue-draggable-resizable": "^2.1.0",
        "vue-fragment": "^1.5.1",
        "vue-slick": "^1.1.15",
